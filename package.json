{
  "name": "@trace/main",
  "version": "0.0.1",
  "description": "Trace logistics system",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/11Clouds/trace.git"
  },
  "keywords": [
    "gps",
    "track"
  ],
  "author": "radioActive DROID <me@godwin.dev>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/11Clouds/trace/issues"
  },
  "homepage": "https://trace.ng",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "client": "pnpm -r --filter ./app/trace.client run",
    "common": "pnpm -r --filter ./app/trace.common run",
    "docs": "pnpm -r --filter ./app/trace.docs run",
    "service": "pnpm -r --filter ./app/trace.service run",
    "website": "pnpm -r --filter ./app/trace.website run",
    "dev": "pnpm -r --filter ./app --parallel run dev",
    "dev:client": "pnpm run client dev",
    "dev:common": "pnpm run common dev",
    "dev:docs": "pnpm run docs dev",
    "dev:service": "pnpm run service dev",
    "build": "pnpm -r --filter ./app run build",
    "clean": " pnpm clean:dist && pnpm clean:packages",
    "clean:packages": "rimraf **/node_modules **/yarn* **/package-lock.* **/.pnpm-debug* **/pnpm-lock.yaml",
    "clean:dist": "rimraf app/**/dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@trace/app": "workspace:*",
    "@trace/client": "workspace:*",
    "@trace/common": "workspace:*",
    "@trace/docs": "workspace:*",
    "@trace/service": "workspace:*",
    "@trace/website": "workspace:*",
    "pnpm": "^6.32.3",
    "rimraf": "^3.0.2"
  },
  "engines": {
    "node": "^18 || ^16 || ^14",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1",
    "pnpm": ">=3"
  }
}
