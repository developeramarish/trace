{
  "extends": "@quasar/app-vite/tsconfig-preset",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "src/*": ["src/*"],
      "@/*": ["src/*"],
      "~/*": ["src/*"],
      "~assets/*": ["src/assets/*"],
      "app/*": ["*"],
      "assets/*": ["src/assets/*"],
      "boot/*": ["src/boot/*"],
      "stores/*": ["src/stores/*"],
      "modules/*": ["src/modules/*"],
      "shared/*": ["src/shared/*"],
      "@shared/*": ["src/shared/*"],
      "@stores/*": ["src/stores/*"],
      "@libs/*": ["src/shared/libs/*"],
      "@layouts/*": ["src/shared/layouts/*"],
      "@components/*": ["src/shared/components/*"],
      "@composables/*": ["src/shared/composables/*"],
      "@direct/*": ["src/apps/direct/*"],
      "@easy/*": ["src/apps/easy/*"],
      "@manager/*": ["src/apps/manager/*"],
      "@vector/*": ["src/apps/vector/*"]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "src/**/*.json",
    "components.d.ts",
    "types.d.ts"
  ]
}
