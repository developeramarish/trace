  "scripts": {
    "setup": "pnpm run rimraf node_modules && pnpm install",
    "add:new": "pnpm --registry=https://registry.npmmirror.com add ",
    "dev": "quasar dev",
    "dev:android": "quasar dev -m capacitor -T android",
    "dev:pwa": "quasar dev -m pwa",
    "dev:ssr": "quasar dev -m ssr",
    "build": "NODE_ENV=production quasar build",
    "build:android": "NODE_ENV=production quasar build -m capacitor -T android",
    "build:pwa": "NODE_ENV=production quasar build -m pwa",
    "build:ssr": "NODE_ENV=production quasar build -m ssr",
    "generate:icon": "icongenie g -i ./src/assets/icon.png --theme-color #3f51b5 --splashscreen-color #3f51b5",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "clean": "pnpm rimraf dist .quasar",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0"
  },
  "dependencies": {
    "@quasar/extras": "^1.13.5",
    "@quasar/quasar-ui-qoverlay": "^2.0.1",
    "@yunzhe35p/vue-fragment": "^1.5.1-vue.3-2",
    "axios": "^0.21.1",
    "dotenv": "^16.0.0",
    "json-stringify-safe": "^5.0.1",
    "pinia": "^2.0.11",
    "pinia-plugin-persistedstate": "^1.5.1",
    "quasar": "^2.6.5",
    "vue": "^3.0.0",
    "vue-i18n": "^9.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
